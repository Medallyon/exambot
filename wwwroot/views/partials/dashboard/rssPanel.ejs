<form action="/dashboard/<%- guild.id %>" method="POST" enctype="application/json">

  <div class="form-check">
    <div class="checkbox">
      <input id="enabled" type="checkbox" class="form-check-input">
      <label for="enabled" class="form-check-label">
        Enabled
      </label>
    </div>
  </div>

  <div class="form-group">
    <label for="feedURL">Feed URL</label>
    <input type="email" class="form-control" id="feedURL" aria-describedby="emailHelp" placeholder="http://files.elderscrollsonline.com/rss/en-us/eso-rss.xml">
    <small id="emailHelp" class="form-text text-muted">Input a valid Feed URL</small>
  </div>

  <div class="form-group">
    <label for="channel">Example select</label>
    <select class="form-control" id="channel" name="channel">
      <% for (let i = 0; i < guild.channels.filter(c => c.type === "text").array().length; i++) { %><option value="<%- guild.channels.filter(c => c.type === "text").array()[i].id %>"># <%- guild.channels.filter(c => c.type === "text").array()[i].name %><% } %></option>
    </select>
  </div>

  <button type="submit" class="btn btn-primary">Submit</button>

</form>